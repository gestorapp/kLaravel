{
    "name": "ksoft/klaravel",
    "version": "1.0.5",
    "description": "A collection of traits and usefull classes and php functions for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiko Seijo",
            "email": "kiko@sunnyface.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ksoft\\Klaravel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "doctrine/dbal": "^2.6",
        "kevupton/ethereal": "v2.3.9.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ksoft\\Klaravel\\ServiceProvider"
            ],
            "aliases": {
                "Larapp": "Ksoft\\Klaravel\\Facade"
            }
        }
    },
    "require-dev": {
        "zircote/swagger-php": "^2.0"
    }
}
